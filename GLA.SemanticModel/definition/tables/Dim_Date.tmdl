	partition Dim_Date = calculated
		mode: import
		source = ```
				ADDCOLUMNS(
				    CALENDARAUTO(),
				    "Année", YEAR([Date]),
				    "Trimestres", QUARTER([Date]),
				    "Mois", FORMAT([Date], "mmm"),
				    "MoisNum", MONTH([Date]),
				    "Aa", RIGHT(YEAR([Date]), 2),
				    
				    -- Colonnes pour l'année fiscale
				    "AnneeFiscale", 
				        IF(
				            MONTH([Date]) >= 10,
				            YEAR([Date]) + 1,
				            YEAR([Date])
				        ),
				    
				    "AnneeFiscaleLabel",
				        IF(
				            MONTH([Date]) >= 10,
				            RIGHT(YEAR([Date]),2) & "/" & RIGHT(YEAR([Date]) + 1, 2),
				            RIGHT(YEAR([Date]) - 1,2) & "/" & RIGHT(YEAR([Date]), 2)
				        ),
				    
				    "TrimestreFiscal",
				        SWITCH(
				            TRUE(),
				            MONTH([Date]) >= 10 && MONTH([Date]) <= 12, 1,
				            MONTH([Date]) >= 1 && MONTH([Date]) <= 3, 2,
				            MONTH([Date]) >= 4 && MONTH([Date]) <= 6, 3,
				            MONTH([Date]) >= 7 && MONTH([Date]) <= 9, 4
				        ),
				    
				    "TrimestreFiscalLabel",
				        "T" & 
				        SWITCH(
				            TRUE(),
				            MONTH([Date]) >= 10 && MONTH([Date]) <= 12, 1,
				            MONTH([Date]) >= 1 && MONTH([Date]) <= 3, 2,
				            MONTH([Date]) >= 4 && MONTH([Date]) <= 6, 3,
				            MONTH([Date]) >= 7 && MONTH([Date]) <= 9, 4
				        ) & " " &
				        IF(
				            MONTH([Date]) >= 10,
				            RIGHT(YEAR([Date]),2) & "/" & RIGHT(YEAR([Date]) + 1, 2),
				            RIGHT(YEAR([Date]) - 1,2) & "/" & RIGHT(YEAR([Date]), 2)
				        ),
				    
				    "MoisFiscal",
				        SWITCH(
				            MONTH([Date]),
				            10, 1,
				            11, 2,
				            12, 3,
				            1, 4,
				            2, 5,
				            3, 6,
				            4, 7,
				            5, 8,
				            6, 9,
				            7, 10,
				            8, 11,
				            9, 12
				        ),
				    
				    "SemestreFiscal",
				        IF(
				            MONTH([Date]) >= 10 || MONTH([Date]) <= 3,
				            1,
				            2
				        ),
				    
				    "SemestreFiscalLabel",
				        "S" & 
				        IF(
				            MONTH([Date]) >= 10 || MONTH([Date]) <= 3,
				            1,
				            2
				        ) & " " &
				        IF(
				            MONTH([Date]) >= 10,
				            RIGHT(YEAR([Date]),2) & "/" & RIGHT(YEAR([Date]) + 1, 2),
				            RIGHT(YEAR([Date]) - 1,2) & "/" & RIGHT(YEAR([Date]), 2)
				        ),
				    
				    "DebutAnneeFiscale",
				        DATE(
				            IF(MONTH([Date]) >= 10, YEAR([Date]), YEAR([Date]) - 1),
				            10,
				            1
				        ),
				    
				    "FinAnneeFiscale",
				        DATE(
				            IF(MONTH([Date]) >= 10, YEAR([Date]) + 1, YEAR([Date])),
				            9,
				            30
				        )
				)
				```

